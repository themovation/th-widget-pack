                <field type="Google Map: Text" editor_type="LINE">link_1_text</field>
                <field type="Google Map: URL" editor_type="LINK" key_of="url">link_1_url</field>
                <field type="Google Map: Text" editor_type="LINE">link_2_text</field>
                <field type="Google Map: URL" editor_type="LINK" key_of="url">link_2_url</field>
            </fields>
