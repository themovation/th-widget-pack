            </fields>
        </widget>

        <!-- Expand List -->
        <widget name="themo-expand-list">
            <fields-in-item items_of="tab_title">
                <field type="Expand List: Tab Title" editor_type="LINE">tab_title</field>
                <field type="Expand List: Tab Content" editor_type="AREA">tab_content</field>
            </fields-in-item>
        </widget>

        <!--  Formidable -->
        <widget name="themo-formidable-form">
            <fields>
