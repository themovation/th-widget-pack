<wpml-config>
    <custom-types>
        <custom-type translate="1">themo_room</custom-type>
        <custom-type translate="1">elementor-thhf</custom-type>
    </custom-types>
    <taxonomies>
        <taxonomy translate="1">themo_room_type</taxonomy>
    </taxonomies>
    <custom-fields>
        <custom-field action="translate">th_room_rating</custom-field>
        <custom-field action="translate">th_room_icons</custom-field>
        <custom-field action="translate">th_room_highlight</custom-field>
        <custom-field action="translate">th_room_title</custom-field>
        <custom-field action="translate">th_room_intro</custom-field>
        <custom-field action="translate">th_room_price</custom-field>
        <custom-field action="translate">th_room_price_per</custom-field>
        <custom-field action="translate">th_room_location</custom-field>
        <custom-field action="translate">th_room_location_link</custom-field>
        <custom-field action="translate">th_room_button_text</custom-field>
    </custom-fields>
    <elementor-widgets>
        <!-- Availability Calendar MPHB -->
        <widget name="themo-mphb-availability-calendar">
            <fields>
                <field type="Calendar: Tooltip Title" editor_type="LINE">tooltip_title</field>
                <field type="Calendar: Accommodation ID" editor_type="LINE">type_id</field>
            </fields>
        </widget>

        <!-- Accommodation Listing -->
        <widget name="themo-accommodation-listing">
            <fields-in-item items_of="listings">
                <field type="Listing: Title" editor_type="LINE">thmv_title</field>
                <field type="Listing: Highlight" editor_type="LINE">thmv_highlight</field>
                <field type="Listing: Preface" editor_type="LINE">thmv_preface</field>
                <field type="Listing: Description" editor_type="AREA">thmv_description</field>
                <field type="Listing: Location Text" editor_type="LINE">thmv_location_text</field>
                <field type="Listing: Location Link" editor_type="LINK">thmv_location_link</field>
                <field type="Listing: Price" editor_type="LINE">thmv_price</field>
                <field type="Listing: Price (before)" editor_type="LINE">thmv_price_before</field>
                <field type="Listing: Price (after)" editor_type="LINE">thmv_price_after</field>

                <field type="Listing: Icon Label" editor_type="LINE">thmv_icon_label0</field>
                <field type="Listing: Icon Labe2" editor_type="LINE">thmv_icon_label1</field>
                <field type="Listing: Icon Labe3" editor_type="LINE">thmv_icon_label2</field>
                <field type="Listing: Icon Labe4" editor_type="LINE">thmv_icon_label3</field>
                <field type="Listing: Icon Labe5" editor_type="LINE">thmv_icon_label4</field>
                <field type="Listing: Icon Labe6" editor_type="LINE">thmv_icon_label5</field>
                <field type="Listing: Icon Labe7" editor_type="LINE">thmv_icon_label6</field>
                <field type="Listing: Icon Labe8" editor_type="LINE">thmv_icon_label7</field>
                <field type="Listing: Icon Labe9" editor_type="LINE">thmv_icon_label8</field>
                <field type="Listing: Icon Label0" editor_type="LINE">thmv_icon_label9</field>
                <field type="Listing: Icon Label1" editor_type="LINE">thmv_icon_label10</field>
                <field type="Listing: Icon Label2" editor_type="LINE">thmv_icon_label11</field>
            </fields-in-item>
        </widget>
        <!-- Blog widget-->
        <widget name="themo-blog">
            <fields>
                <field type="Blog: Read more" editor_type="LINE">thmv_link_text</field>
            </fields>
            <fields-in-item items_of="listings">
                <field type="Blog - Listing: Read more" editor_type="LINE">thmv_link_text</field>
            </fields-in-item>    
        </widget>
        <!-- Booked Appointment Calendar -->
        <widget name="themo-appointments">
            <fields>
                <field type="Appointments: Tooltip Title" editor_type="LINE">tooltip_title</field>
                <field type="Appointments: Shortcode" editor_type="LINE">calendar_shortcode</field>
            </fields>
        </widget>

        <!-- Button -->
        <widget name="themo-button">
            <fields>
                <field type="Button: Text 1" editor_type="LINE">button_1_text</field>
                <field type="Button: URL 1" editor_type="LINK" key_of="url">button_1_link</field>
                <field type="Button: Text 2" editor_type="LINE">button_2_text</field>
                <field type="Button: URL 2" editor_type="LINK" key_of="url">button_2_link</field>
            </fields>
        </widget>

        <!-- Call to Action -->
        <widget name="themo-call-to-action">
            <fields>
                <field type="Call to action: Text" editor_type="LINE">text</field>
                <field type="Call to action: Button 1 Text" editor_type="LINE">button_1_text</field>
                <field type="Call to action: Button 1 URL" editor_type="LINK" key_of="url">button_1_link</field>
